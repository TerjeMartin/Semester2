{
  "name": "semester-project-2",
  "version": "0.0.2",
  "description": "Noroff School of technology and digital media auction house website.",
  "main": "src/js/index.js",
  "scripts": {
    "build": "sass styles/scss:styles/css",
    "watch": "sass --watch styles/scss:styles/css",
    "format": "prettier -w src/**/*.js",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TerjeMartin/Semester2.git"
  },
  "keywords": [
    "e-commerce",
    "portfolio",
    "website",
    "bootstrap",
    "sass"
  ],
  "author": "Ilya Martchenko <iliamartchenko@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TerjeMartin/Semester2/issues"
  },
  "homepage": "https://github.com/TerjeMartin/Semester2#readme",
  "devDependencies": {
    "eslint": "^8.38.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "sass": "^1.62.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "bootstrap": "^5.3.0-alpha3"
  }
}
